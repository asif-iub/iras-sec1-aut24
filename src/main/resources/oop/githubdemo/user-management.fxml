<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.githubdemo.UserManagementController">
    <Label layoutX="75.0" layoutY="14.0" text="User Management" AnchorPane.leftAnchor="75.0" AnchorPane.topAnchor="14.0">
        <font>
            <Font size="36.0" />
        </font>
    </Label>
    <Button layoutX="468.0" layoutY="15.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log Out" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="15.0">
        <font>
            <Font size="24.0" />
        </font>
    </Button>
    <Button layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="←" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="15.0">
        <font>
            <Font size="24.0" />
        </font>
    </Button>
   <TableView fx:id="userTableView" layoutX="198.0" layoutY="103.0" prefHeight="283.0" prefWidth="390.0">
     <columns>
       <TableColumn fx:id="usernameTC" prefWidth="127.0" text="Username" />
       <TableColumn fx:id="passwordTC" prefWidth="168.0" text="Password" />
         <TableColumn fx:id="ageTC" prefWidth="89.0" text="Age" />
     </columns>
   </TableView>
   <TextField fx:id="usernameTF" layoutX="14.0" layoutY="102.0" prefHeight="51.0" prefWidth="162.0" promptText="Username">
      <font>
         <Font size="24.0" />
      </font>
   </TextField>
   <TextField fx:id="passwordTF" layoutX="14.0" layoutY="175.0" prefHeight="51.0" prefWidth="162.0" promptText="Password">
      <font>
         <Font size="24.0" />
      </font>
   </TextField>
   <TextField fx:id="ageTF" layoutX="16.0" layoutY="243.0" prefHeight="51.0" prefWidth="162.0" promptText="Age">
      <font>
         <Font size="24.0" />
      </font>
   </TextField>
   <Button layoutX="16.0" layoutY="321.0" mnemonicParsing="false" onAction="#onAddUserButtonClick" prefHeight="51.0" prefWidth="162.0" text="Add User">
      <font>
         <Font size="24.0" />
      </font>
   </Button>
</AnchorPane>
