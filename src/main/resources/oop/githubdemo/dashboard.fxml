<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.githubdemo.Dashboard">
   <children>
      <Label layoutX="14.0" layoutY="22.0" text="Welcome to user dashboard." AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="22.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button layoutX="468.0" layoutY="14.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log Out" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="87.0" mnemonicParsing="false" onAction="#goToProfile" prefHeight="129.0" prefWidth="135.0" text="Profile">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="166.0" layoutY="87.0" mnemonicParsing="false" onAction="#goToUserManagement" prefHeight="129.0" prefWidth="339.0" text="User Management">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="230.0" mnemonicParsing="false" onAction="#goToChart" prefHeight="129.0" prefWidth="135.0" text="Chart">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="166.0" layoutY="230.0" mnemonicParsing="false" onAction="#goToPDF" prefHeight="129.0" prefWidth="135.0" text="PDF">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
