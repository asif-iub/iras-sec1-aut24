<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.githubdemo.raghibul.PdfController">
   <children>
      <Label layoutX="75.0" layoutY="14.0" text="PDF">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button layoutX="468.0" layoutY="15.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log Out">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="←">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="212.0" layoutY="175.0" mnemonicParsing="false" onAction="#onGeneratePDFButtonClick" text="Generate PDF">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Label fx:id="messageLabel" alignment="CENTER" layoutX="12.0" layoutY="259.0" prefHeight="66.0" prefWidth="579.0" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
