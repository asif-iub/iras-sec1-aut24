<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.githubdemo.raghibul.Profile">
   <children>
      <Label layoutX="75.0" layoutY="14.0" text="Profile" AnchorPane.leftAnchor="75.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="105.0" text="Username: " AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="105.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="11.0" layoutY="175.0" text="Password: " AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="175.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="usernameInput" editable="false" layoutX="151.0" layoutY="97.0" AnchorPane.leftAnchor="151.0" AnchorPane.topAnchor="97.0">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <Button layoutX="468.0" layoutY="15.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log Out" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="←" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="151.0" layoutY="167.0" mnemonicParsing="false" text="Update Password" AnchorPane.leftAnchor="151.0" AnchorPane.topAnchor="167.0">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
