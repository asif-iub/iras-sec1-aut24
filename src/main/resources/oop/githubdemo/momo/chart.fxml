<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.githubdemo.momo.ChartController">
   <children>
      <Label layoutX="85.0" layoutY="24.0" text="Chart" AnchorPane.leftAnchor="75.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button layoutX="478.0" layoutY="25.0" mnemonicParsing="false" onAction="#onLogOutButtonClick" text="Log Out" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="24.0" layoutY="25.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="←" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="15.0">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="468.0" layoutY="79.0" mnemonicParsing="false" onAction="#onLoadButtonClick" prefHeight="51.0" prefWidth="118.0" text="Load" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="79.0">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <LineChart fx:id="chart" layoutX="14.0" layoutY="79.0" prefHeight="307.0" prefWidth="440.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
